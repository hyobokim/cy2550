include <tunables/global>
/home/hyokim/p2 flags=(audit) {
   include <abstractions/base>
   /home/hyokim/p2 mr,
   /tmp/** rw,
   /home/hyokim/.project2/config rw,
   /var/log/syslog rw,
   /etc/fstab r,
   /etc/crontab r,
   /etc/resolv.conf rw,
   /run/systemd/resolve/stub-resolv.conf r,
   /etc/passwd r,
   /etc/hosts r,
   /proc/version rw,
   /proc/buddyinfo rw,
   @{HOME}/template r,
   /var/log/syslog mr,
   network tcp,
}
